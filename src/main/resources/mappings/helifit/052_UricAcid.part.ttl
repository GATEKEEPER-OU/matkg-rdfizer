# ############################################################################ #
#                                 UricAcid
# ############################################################################ #

# @todo Assignment

# 3084-1 UricAcid
<#UricAcidMeasurement> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '3084-1')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/Measurement/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:HLF308UricAcidMeas;
  ];

  #
  # Metadata
  #
  rr:predicateObjectMap [
    rr:predicate ho:P140AssignedAttributeTo;
    rr:objectMap [
      rr:parentTriplesMap <#Patient>;
      rr:joinCondition [
        rr:child "resource.subject.reference";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4HasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#TimeSpan>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  #
  # Link(s)
  #
  rr:predicateObjectMap [
    rr:predicate ho:P40ObservedDimension;
    rr:objectMap [
      rr:parentTriplesMap <#UricAcidDimension>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];
.

# 3084-1 UricAcid
<#UricAcidDimension> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '3084-1')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/Dimension/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:HLF408UricAcidDim;
  ];

  #
  # Data
  #
  rr:predicateObjectMap [
    rr:predicate ho:P91hasUnit;
    rr:objectMap [
      rml:reference "resource.valueQuantity.unit";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P90hasValue;
    rr:objectMap [
      rml:reference "resource.valueQuantity.value";
    ]
  ];
.

# /UricAcid
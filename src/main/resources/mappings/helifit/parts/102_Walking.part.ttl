# ############################################################################ #
#                                  Walking
# ############################################################################ #

#   73985-4 Exercise activity
# LA11834-1 Walking
<#WalkingActivity> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '73985-4')]"
  ];

  rr:subjectMap [
    rr:template "https://opensource.samsung.com/projects/helifit/type/HLF101/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:HLF101Walking;
  ];

  #
  # Metadata
  #
  rr:predicateObjectMap [
    rr:predicate ho:P140assignedAttributeTo;
    rr:objectMap [
      rr:parentTriplesMap <#Patient>;
      rr:joinCondition [
        rr:child "resource.subject.reference";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4hasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#ObservationTimeSpan>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  #
  # Link(s)
  #
  rr:predicateObjectMap [
    rr:predicate ho:P117occursDuring;
    rr:objectMap [
      rr:parentTriplesMap <#DurationMeasurement>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "resourceId";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P117occursDuring;
    rr:objectMap [
      rr:parentTriplesMap <#EnergyMeasurement>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "resourceId";
      ];
    ];
  ];
.

# 55411-3 Exercise duration
<#DurationMeasurement> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[*].resource.component[?(@.code.coding[0].code == '55411-3')]"
  ];

  rr:subjectMap [
    rr:template "https://opensource.samsung.com/projects/helifit/type/HLF188/code/{code.coding[0].code}/id/{resourceId}";
    rr:class ho:HLF188DurationMeasurement;
  ];

  #
  # Metadata
  #
  #rr:predicateObjectMap [
  #  rr:predicate ho:P140assignedAttributeTo;
  #  rr:objectMap [
  #    rr:parentTriplesMap <#Patient>;
  #    rr:joinCondition [
  #      rr:child "resource.subject.reference";
  #      rr:parent "fullUrl";
  #    ];
  #  ];
  #];

  rr:predicateObjectMap [
    rr:predicate ho:P4hasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#ObservationTimeSpan>;
      rr:joinCondition [
        rr:child "resourceId";
        rr:parent "fullUrl";
      ];
    ];
  ];

  #
  # Link(s)
  #
  rr:predicateObjectMap [
    rr:predicate ho:P40ObservedDimension;
    rr:objectMap [
      rr:parentTriplesMap <#DurationDimension>;
      rr:joinCondition [
        rr:child "resourceId";
        rr:parent "resourceId";
      ];
    ];
  ];
.


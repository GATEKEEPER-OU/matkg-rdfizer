# 73985-4 Exercise activity
# LA11839-0 WeightLifting
<#WeightLiftingActivity> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == 'LA11839-0')]"
  ];

  rr:subjectMap [
    rr:template "https://opensource.samsung.com/projects/helifit/type/HLF133/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:HLF133WeightLifting;
  ];

  #
  # Metadata
  #
  rr:predicateObjectMap [
    rr:predicate ho:P1isIdentifiedBy;
    rr:objectMap [
      rr:parentTriplesMap <#TemportalEntityIdentifier>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P14carriedOutBy;
    rr:objectMap [
      rr:parentTriplesMap <#Patient>;
      rr:joinCondition [
        rr:child "resource.subject.reference";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4hasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#ObservationTimeSpan>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  #
  # Link(s)
  #
  rr:predicateObjectMap [
    rr:predicate ho:P117includes;
    rr:objectMap [
      rr:parentTriplesMap <#DurationMeasurement>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "resourceId";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P117includes;
    rr:objectMap [
      rr:parentTriplesMap <#EnergyMeasurement>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "resourceId";
      ];
    ];
  ];
.


<#ICalendar> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Appointment' && @.resource.appointmentType.coding[0].code == 'iCalendar')]"
  ];

  rr:subjectMap [
    rr:template "https://opensource.samsung.com/projects/helifit/type/HLF900/code/{resource.appointmentType.coding[0].code}/id/{resource.id}";
    rr:class ho:HLF900iCalendarObject;
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P67refersTo;
    rr:objectMap [
      rr:parentTriplesMap <#Patient>;
      rr:joinCondition [
        rr:child "resource.participant[0].actor.display";
        rr:parent "resource.id";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P129isAbout;
    rr:objectMap [
      rr:parentTriplesMap <#CalendarEvent>;
      rr:joinCondition [
        rr:child "resource.id";
        rr:parent "resource.supportingInformation[0].display";
      ];
    ];
  ];
.

<#CalendarEvent> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Appointment' && @.resource.appointmentType.coding[0].code != 'iCalendar')]"
  ];

  rr:subjectMap [
    rr:template "https://opensource.samsung.com/projects/helifit/type/HLF901/code/{resource.appointmentType.coding[0].code}/id/{resource.id}";
    rr:class ho:HLF901CalendarEvent;
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P2hasType;
    rr:objectMap [
      rr:parentTriplesMap <#AppointmentEntityIdentifier>;
      rr:joinCondition [
        rr:child "resource.id";
        rr:parent "resource.id";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P7tookPlaceAt;
    rr:objectMap [
      rr:parentTriplesMap <#CalendarLocation>;
      rr:joinCondition [
        rr:child "resource.id";
        rr:parent "resource.id";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4hasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#AppointmentDateTime>;
      rr:joinCondition [
        rr:child "resource.id";
        rr:parent "resource.id";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P3hasNote;
    rr:objectMap [
      rml:reference "resource.extension[?(@.url == 'SUMMARY')].valueString";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:EPXXXhasCalendarScale;
    rr:objectMap [
      rml:reference "resource.extension[?(@.url == 'CALSCALE')].valueString";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:EPXXXhasStatus;
    rr:objectMap [
      rml:reference "resource.extension[?(@.url == 'STATUS')].valueString";
    ]
  ];
.
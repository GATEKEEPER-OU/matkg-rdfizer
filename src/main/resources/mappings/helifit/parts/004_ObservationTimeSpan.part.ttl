# ############################################################################ #
#                                  ObservationTimeSpan
# ############################################################################ #

<#ObservationTimeSpan> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/TimeSpan/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:E52TimeSpan;
  ];

  rr:predicateObjectMap [
    rr:predicate ho:HLFP04effectivedatatime;
    rr:objectMap [
      rml:reference "resource.effectiveDateTime";
      rr:datatype xsd:dataTime
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:FWP02Starts;
    rr:objectMap [
      rml:reference "resource.valuePeriod.start";
      rr:datatype xsd:dataTime
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:FWP03Ends;
    rr:objectMap [
      rml:reference "resource.valuePeriod.end";
      rr:datatype xsd:dataTime
    ]
  ];
.

# /ObservationTimeSpan
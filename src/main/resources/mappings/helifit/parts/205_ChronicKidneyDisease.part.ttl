# ############################################################################ #
#                                ChronicKidneyDisease
# ############################################################################ #

# DOID_784 Chronic kidney disease
<#ChronicKidneyDiseaseAssignment> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Condition' && @.resource.code.coding[0].code == 'DOID_784')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/HLF600/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:HLF600DiseaseAss;
  ];

  #
  # Metadata
  #
  rr:predicateObjectMap [
    rr:predicate ho:P140AssignedAttributeTo;
    rr:objectMap [
      rr:parentTriplesMap <#Patient>;
      rr:joinCondition [
        rr:child "resource.subject.reference";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4HasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#ConditionTimeSpan>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  #
  # Link(s)
  #
  rr:predicateObjectMap [
    #rr:predicate ho:P141Assigned;
    rr:predicate ho:P140AssignedAttributeTo;
    rr:objectMap [
      rr:parentTriplesMap <#ChronicKidneyDiseaseCourse>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  #
  # Data
  #
  rr:predicateObjectMap [
    rr:predicate ho:HLFP05assignedPresence;
    rr:objectMap [
      rml:reference "resource.code.text";
    ]
  ];
.


# ############################################################################ #
#                                 LowDensityLipoprotein
# ############################################################################ #

# @todo Assignment

# 2089-1 Low Density Lipoprotein
<#LowDensityLipoproteinMeasurement> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '2089-1')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/Measurement/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:HLF303LowDensityLipoproteinMeas;
  ];

  #
  # Metadata
  #
  rr:predicateObjectMap [
    rr:predicate ho:P140AssignedAttributeTo;
    rr:objectMap [
      rr:parentTriplesMap <#Patient>;
      rr:joinCondition [
        rr:child "resource.subject.reference";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4HasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#TimeSpan>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  #
  # Link(s)
  #
  rr:predicateObjectMap [
    rr:predicate ho:P40ObservedDimension;
    rr:objectMap [
      rr:parentTriplesMap <#LowDensityLipoproteinDimension>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];
.

# 2089-1 Low Density Lipoprotein
<#LowDensityLipoproteinDimension> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '2089-1')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/Dimension/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:HLF403LowDensityLipoproteinDim;
  ];

  #
  # Data
  #
  rr:predicateObjectMap [
    rr:predicate ho:P91hasUnit;
    rr:objectMap [
      rml:reference "resource.valueQuantity.unit";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P90hasValue;
    rr:objectMap [
      rml:reference "resource.valueQuantity.value";
    ]
  ];
.

# /LowDensityLipoprotein
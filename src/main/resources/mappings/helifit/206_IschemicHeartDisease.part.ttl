  #
  # Link(s)
  #
  rr:predicateObjectMap [
    rr:predicate ho:HP5realizes;
    rr:objectMap [
      rr:parentTriplesMap <#IschemicHeartDesease>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];
.

# DOID_3393 Ischemic Heart Disease
<#IschemicHeartDesease> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Condition' && @.resource.code.coding[0].code == 'DOID_3393')]"
  ];

  rr:subjectMap [
    rr:template "http://purl.obolibrary.org/obo/{resource.code.coding[0].code}";
    rr:class doid:DOID_9452;
  ];

  #
  # Data
  #
  rr:predicateObjectMap [
    rr:predicate ho:P91hasUnit;
    rr:objectMap [
      rml:reference "resource.code.text";
    ]
  ];
.

# /IschemicHeartDisease
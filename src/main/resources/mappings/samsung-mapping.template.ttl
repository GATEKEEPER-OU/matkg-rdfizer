@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix ho: <http://www.samsung.com/health/ontology#> .
@base <http://www.samsung.com/health> .


# ############################################################################ #
#                                  Patient
# ############################################################################ #

<#Patient> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Patient')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/id/{resource.identifier[0].value}";
    rr:class ho:E21Person;
  ];
.


# ############################################################################ #
#                                  TimeSpan
# ############################################################################ #

# 85354-9 Vital Signs
# 73985-4 Exercise activity
#  8867-4 Heart rate
#  8302-2 Body height
# 20564-1 Oxygen saturation in Blood
# 93832-4 Sleep duration
# IGNORED
# LP412113-5 Light sleep duration
# 93830-8 Light sleep duration
# 93831-6 Deep sleep duration
<#TimeSpan> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && (@.resource.code.coding[0].code == '85354-9' || @.resource.code.coding[0].code == '73985-4' || @.resource.code.coding[0].code == '8867-4' || @.resource.code.coding[0].code == '8302-2' || @.resource.code.coding[0].code == '20564-1' || @.resource.code.coding[0].code == '93832-4'))]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/TimeSpan/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:E52TimeSpan;
  ];

  rr:predicateObjectMap [
    rr:predicate ho:FWP02Starts;
    rr:objectMap [
      rml:reference "resource.valuePeriod.start";
      rr:datatype xsd:dataTime
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:FWP03Ends;
    rr:objectMap [
      rml:reference "resource.valuePeriod.end";
      rr:datatype xsd:dataTime
    ]
  ];
.



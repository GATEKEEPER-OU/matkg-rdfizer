# ############################################################################ #
#                                Respiration
# ############################################################################ #

# 20564-1 Oxygen saturation in Blood
<#RespirationAss> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '20564-1')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/Assignment/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:FWE166RespirationAss;
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P140AssignedAttributeTo;
    rr:objectMap [
      rr:parentTriplesMap <#Patient>;
      rr:joinCondition [
        rr:child "resource.subject.reference";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P141Assigned;
    rr:objectMap [
      rr:parentTriplesMap <#RespirationMeas>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4HasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#TimeSpan>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];
.

# 20564-1 Oxygen saturation in Blood
<#RespirationMeas> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '20564-1')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/Measurement/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:FWE179RespirationMeas;
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P40ObservedDimension;
    rr:objectMap [
      rr:parentTriplesMap <#OxygenSaturationDim>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P4HasTimeSpan;
    rr:objectMap [
      rr:parentTriplesMap <#TimeSpan>;
      rr:joinCondition [
        rr:child "fullUrl";
        rr:parent "fullUrl";
      ];
    ];
  ];
.

# 20564-1 Oxygen saturation in Blood
<#OxygenSaturationDim> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "__RML_SRC__";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.entry[?(@.resource.resourceType == 'Observation' && @.resource.code.coding[0].code == '20564-1')]"
  ];

  rr:subjectMap [
    rr:template "http://www.samsung.com/health/data/type/Dimension/code/{resource.code.coding[0].code}/id/{fullUrl}";
    rr:class ho:FWE27SpO2Dim;
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P91hasUnit;
    rr:objectMap [
      rml:reference "resource.valueQuantity.unit";
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate ho:P90hasValue;
    rr:objectMap [
      rml:reference "resource.valueQuantity.value";
    ]
  ];
.

# /Respiration